{{#each this}}
<a href="#" class="list-group-item px-0">
    <div class="media align-items-center">
        <div class="position-relative">
            <img src="{{from.picture}}" class="rounded-circle mr-2 shadow" alt="">
            <span class="status {{from.status}}"></span>
        </div>

        <div class="media-body small">
            <p class="mb-0 d-flex w-100 justify-content-between">
                <span class="bold mb-1 text-capitalize">{{from.name}}</span>
            </p>
            <p class="small color-2 alpha-5 mb-2">{{from.email}}</p>
            <p class="mb-0 color-2">{{message}}</p>
        </div>
    </div>
</a>
{{/each}}
